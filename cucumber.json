{
    "default":{
        "formatOptions":{
            "snippetInterface":"async-await"
        },
        "paths":[
            "src/test/features/"
        ],
        "publishQuiet": false,
        "dryRun":false,
        "require":[
            "src/test/steps/*.ts"
        ],
        "requireModule":[
            "ts-node/register"
        ],
        "parallel":2,
        "format":[
            "progress-bar",
            "html:test-results/cucumber-report.html",
            "json:test-results/cucumber-report.json"
        ]
    }
}